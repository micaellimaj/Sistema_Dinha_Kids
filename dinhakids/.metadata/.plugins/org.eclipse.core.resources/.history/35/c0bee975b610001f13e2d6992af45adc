package com.example.dinhakids.sistemaweb.Models;


public class Task {
    
    public Task(Long id, String nomes, String emails, String senhas){
        this.id = id;
        this.nomes = nomes;
        this.emails = emails;
        this.senhas = senhas;
    }

    public Task() {

    }

    private Long id;
    private String nomes;
    private String emails;
    private String senhas;

    public void setId(Long id) {
        this.id = id;
    }
    public Long getId() {
        return id;
    }

    public void setNomes(String nomes) {
        this.nomes = nomes;
    }

    public String getNomes(){
        return nomes;
    }

    public void setEmails(String emails) {
        this.emails = emails;
    }

    public String getEmails(){
        return emails;
    }

    public void setSenhas(String Senhas) {
        this.senhas = Senhas;
    }

    public String getSenhas(){
        return senhas;
    }
}
